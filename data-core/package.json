{
    "name": "@fgo-planner/data-core",
    "version": "0.0.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/fgo-planner/npm-lib-data.git"
    },
    "author": "Alvin Quach",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fgo-planner/npm-lib-data/issues"
    },
    "homepage": "https://github.com/fgo-planner/npm-lib-data/tree/master/data-core#readme",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "main": "lib/cjs/index.js",
    "module": "lib/esm/index.js",
    "types": "lib/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./lib/types/index.d.ts",
            "require": "./lib/cjs/index.js",
            "import": "./lib/esm/index.js"
        },
        "./constants": {
            "types": "./lib/types/constants/index.d.ts",
            "require": "./lib/cjs/constants/index.js",
            "import": "./lib/esm/constants/index.js"
        },
        "./types": {
            "types": "./lib/types/types/index.d.ts",
            "require": "./lib/cjs/types/index.js",
            "import": "./lib/esm/types/index.js"
        },
        "./utils": {
            "types": "./lib/types/utils/index.d.ts",
            "require": "./lib/cjs/utils/index.js",
            "import": "./lib/esm/utils/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "lib/types/index.d.ts"
            ],
            "constants": [
                "lib/types/constants/index.d.ts"
            ],
            "types": [
                "lib/types/types/index.d.ts"
            ],
            "utils": [
                "lib/types/utils/index.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "test": "jest --coverage --verbose",
        "test:build": "jest --silent --noStackTrace --forceExit",
        "lint": "eslint \"**/*.{js,ts}\"",
        "clean": "rimraf lib",
        "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --no-comments",
        "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --no-comments",
        "declarations": "tsc -p tsconfig.json",
        "build": "npm-run-all -l lint -p test:build -p clean -p build:esm build:cjs declarations",
        "prepublishOnly": "npm i && npm run build"
    },
    "jest": {
        "roots": [
            "test"
        ],
        "testPathIgnorePatterns": [
            "resources"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        }
    },
    "dependencies": {
        "@fgo-planner/common-core": "^0.0.1",
        "@fgo-planner/data-types": "^0.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.18.13",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-typescript": "^7.18.6",
        "@fgo-planner/data-test-resources": "^0.0.1",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.15",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.21.0",
        "jest": "^28.1.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.8",
        "typescript": "^4.7.4"
    }
}
